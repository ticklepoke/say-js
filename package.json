{
	"name": "@ticklepoke/sayjs",
	"version": "1.0.0",
	"description": "A call graph generator for js",
	"main": "index.js",
	"author": "Nigel Lee",
	"license": "MIT",
	"private": false,
	"publishConfig": {
		"access": "public"
	},
	"repository": "https://github.com/ticklepoke/say-js",
	"scripts": {
		"lint": "eslint ./**/*.ts",
		"lint:fix": "eslint --fix ./**/*.ts",
		"pretest": "gulp dev",
		"test": "jest dist/",
		"test:watch": "nodemon -e ts -i dist --exec 'gulp dev && jest' dist/",
		"dev": "ts-node -r tsconfig-paths/register driver/sayjs.ts",
		"deps:check": "dpdm --circular --exit-code circular:1 ./**/*.ts",
		"docs:diagrams:gen": "./scripts/gen-diagrams.sh",
		"types:check": "tsc --noEmit",
		"tsc": "tsc",
		"gulp": "gulp",
		"semantic-release": "semantic-release"
	},
	"devDependencies": {
		"@babel/cli": "^7.14.8",
		"@babel/core": "^7.15.0",
		"@babel/preset-env": "^7.15.0",
		"@babel/preset-typescript": "^7.15.0",
		"@mermaid-js/mermaid-cli": "^8.11.0",
		"@types/esprima": "^4.0.3",
		"@types/jest": "^27.0.1",
		"@types/node": "^16.4.13",
		"@typescript-eslint/eslint-plugin": "^4.29.0",
		"@typescript-eslint/parser": "^4.29.0",
		"babel-plugin-root-import": "^6.6.0",
		"dpdm": "^3.7.1",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^3.4.0",
		"fp-ts-std": "^0.11.0",
		"gulp": "^4.0.2",
		"gulp-babel": "^8.0.0",
		"gulp-typescript": "^6.0.0-alpha.1",
		"husky": "^7.0.1",
		"jest": "^27.0.6",
		"lint-staged": "^11.1.1",
		"nodemon": "^2.0.12",
		"prettier": "^2.3.2",
		"ts-jest": "^27.0.4",
		"ts-node": "^10.1.0",
		"tsconfig-paths": "^3.10.1",
		"typescript": "^4.3.5"
	},
	"dependencies": {
		"@semantic-release/changelog": "^6.0.0",
		"@semantic-release/commit-analyzer": "^9.0.1",
		"@semantic-release/git": "^10.0.0",
		"@semantic-release/github": "^8.0.0",
		"@semantic-release/npm": "^8.0.0",
		"argparse": "^2.0.1",
		"ast-types": "^0.14.2",
		"esprima": "^4.0.1",
		"fp-ts": "^2.11.1"
	},
	"bin": {
		"sayjs": "./driver/cli.js"
	}
}
